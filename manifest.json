{
    "name": "NoNecro",
    "version": "1.0",
    "description": "Filters out necroposts on most internet forums.",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ]
}